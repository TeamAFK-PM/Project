    <!-- bradcam_area  -->
    <div class="bradcam_area breadcam_bg_2">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="bradcam_text ">
                        
                        <form class="form-inline my-2 my-lg-0 my-lg-0 justify-content-center" style="display: flex !important; ">
                            <input class="form-control mr-sm-2 fs-17 sizefull" type="text" name="name"
                                placeholder="Nhập tên cầu thủ" aria-label="Search"
                                style="width: 80%; height: 55px; background: #f1f1f1; border: 1px solid #696464;">
                            <button class="btn btn-info my-2 my-sm-0 fs-17"
                                style="width: 86px; height: 55px; border: 1px solid grey; background: #2196F3; color:white"
                                type="submit"><i class="fa fa-search"></i></button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ bradcam_area  -->
    <!-- music_area  -->
    <%if (search_name.recordset.length > 0) { %>
        <% for(var i = 0; i < search_name.recordset.length; i++) { %>
            <div class="music_area music_gallery inc_padding">
                <div class="container">
                    <div class="row align-items-center justify-content-center mb-20">
                        <div class="col-xl-10">
                            <div class="row align-items-center">
                                        <div class="col-xl-9 col-md-9">
                                            <div class="music_field">
                                                    <div class="thumb">
                                                            <img src="img/music_man/1.png" alt="">
                                                        </div>
                                                        <div class="audio_name">
                                                            <div class="name">                 
                                                                <h4><%= search_name.recordset[i].HoTen%></h4>
                                                                <p><%= search_name.recordset[i].NgaySinh%></p>
                                                            </div>
                                                               
                                                        </div>
                                            </div>
                                        </div>
                                        <div class="col-xl-3 col-md-3">
                                            <div class="music_btn">
                                                    <a href="/playerinfor?name=<%= search_name.recordset[i].HoTen %>" class="boxed-btn">More info</a>
                                            </div>
                                        </div>
                            </div>
                        </div>
        
                    </div>
                </div>
            </div>
            <% } %>
        <% } %>
        <%if (search_name.recordset.length == 0) { %>
            <% for(var i = start; i < end; i++) { %>
                <div class="music_area music_gallery inc_padding">
                    <div class="container">
                        <div class="row align-items-center justify-content-center mb-20">
                            <div class="col-xl-10">
                                <div class="row align-items-center">
                                            <div class="col-xl-9 col-md-9">
                                                <div class="music_field">
                                                        <div class="thumb">
                                                                <img src="img/music_man/1.png" alt="">
                                                            </div>
                                                            <div class="audio_name">
                                                                <div class="name">                 
                                                                    <h4><%= results.recordset[i].HoTen%></h4>
                                                                    <p><%= results.recordset[i].NgaySinh%></p>
                                                                </div>
                                                                   
                                                            </div>
                                                </div>
                                            </div>
                                            <div class="col-xl-3 col-md-3">
                                                <div class="music_btn">
                                                        <a href="/playerinfor?name=<%= results.recordset[i].HoTen %>" class="boxed-btn">More info</a>
                                                </div>
                                            </div>
                                </div>
                            </div>
            
                        </div>
                    </div>
                </div>
                <% } %>
                <ul class="pagination pagination-lg justify-content-center" style="margin:20px 0">
                    <li class="page-item"><a class="page-link" href="/player?page=1">1</a></li>
