    <section class="ftco-section ftco-about">
        <div class="container">
            <form action="edittournament" method="POST">
            <div class="bg-white rounded-lg shadow p-4">

                <div class="input-group flex-nowrap">
                    <div class="input-group-prepend">
                        <span class="input-group-text" id="addon-wrapping">Time</span>
                    </div>
                    <input type="text" class="form-control" style="outline: auto;" placeholder="Username"
                        aria-label="Username" aria-describedby="addon-wrapping">
                </div>

                <!-- Progress bar 1 -->
                <!-- END -->

                <!-- Demo info -->
                <div class="row text-center mt-4" style="width : px; margin-left: 0px !important;">
                    <div class="col-6 border-right"> 
                        <div class= "d-flex" style="justify-content: flex-start;">
                            <img src="./img/about.jpg" alt="logo" style="width : 100px; height :100px"
                                class="figure float-left m-r-15">
                            <div class="form-group" style="width: 150px;">
                                <label for="exampleFormControlSelect1">Name of player</label>
                                <select class="form-control" id="exampleFormControlSelect1">
                                    <option><%= result.HT1%></option>
                                </select>
                            </div>
                        </div>
                        <div class="input-group flex-nowrap">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="addon-wrapping">Score</span>
                            </div>
                            <% if(result.HT1 == result.HT3){ %>
                                <input type="text" name = "score1" class="form-control" style="outline: auto;"  placeholder="1"
                                aria-label="Username" aria-describedby="addon-wrapping">
                            <% } else { %>
                                <input type="text" name = "score1" class="form-control" style="outline: auto;"  placeholder="0"
                                aria-label="Username" aria-describedby="addon-wrapping">
                            <% } %>
                        </div>
                    </div>
                    <div class="col-6 border-left">
                        <div class="float-right figure" style="width: 100%;">
                            <div class= "d-flex" style="justify-content: flex-end;">
                                <div class="form-group" style="width: 150px;">
                                    <label for="exampleFormControlSelect1">Name of player</label>
                                    <select class="form-control" id="exampleFormControlSelect1">
                                        <option><%= result.HT2%></option>
                                    </select>
                                </div>
                                <img src="./img/about.jpg" alt="logo" style="width : 100px; height :100px"
                                class="figure m-l-15">
                            </div>
                            <div class="input-group flex-nowrap">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="addon-wrapping">Score</span>
                                </div>
                                <% if(result.HT2 == result.HT3){ %>
                                    <input type="text" name = "score2" class="form-control" style="outline: auto;"  placeholder="1"
                                    aria-label="Username" aria-describedby="addon-wrapping">
                                <% } else { %>
                                    <input type="text" name = "score2" class="form-control" style="outline: auto;"  placeholder="0"
                                    aria-label="Username" aria-describedby="addon-wrapping">
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- END -->
            </div>
            <div style="height: 25px;"></div>
            <div class = "d-flex" style="justify-content: flex-end;">
                <a class="btn btn-danger w-80" style="margin-right:10px;" href="" role="button"><i class="fa fa-backward" aria-hidden="true"></i> Back</a>
                <button type="submit" class="btn btn-outline-primary float-right">Save</button>
            </div>
        </form>
